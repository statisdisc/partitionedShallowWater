FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceVectorField;
    location    "0";
    object      Uf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    equator
    {
        type            zeroGradient;
        //value           uniform (100 0 0);
    }
    pole
    {
        type            zeroGradient;
        //value           uniform (100 0 0);
    }
    inlet
    {
        type            zeroGradient;
        //value           uniform (100 0 0);
    }
    outlet
    {
        type            zeroGradient;
        //value           uniform (100 0 0);
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


