FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

implicitAdvection false;
implicitSource true;
CrankNicolsonAlpha 1;

solvers
{
    "(rt|rl|rv)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-08
        relTol          0;
    }
}

