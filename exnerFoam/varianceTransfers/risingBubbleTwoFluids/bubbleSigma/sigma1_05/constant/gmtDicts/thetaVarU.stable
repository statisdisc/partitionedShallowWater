FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X12c/12c";
projection2D XZ;

FieldsToPlot
(
    [thetaVar.stable filledContours 0 0.5 0.01 "$GMTU/colours/white_black.cpt"]
    //[u.stable    vectors 50 31 "black"]
);

separateLegend true;
region "-5000/5000/0/10000";
annotate "minMax 250 350";
boundaryMarks "2500/2500";
plotBoundaryDots false;
