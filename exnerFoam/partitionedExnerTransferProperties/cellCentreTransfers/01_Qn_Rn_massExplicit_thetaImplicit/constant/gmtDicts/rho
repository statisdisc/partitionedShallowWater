FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/18c";
projection2D XZ;

FieldsToPlot
(
    [rhoDiff filledContours -5e-3 5e-3 5e-4 "$GMTU/colours/red_white_blue.cpt"]
);

//separateLegend true;
region "-5000/5000/0/1e4";
annotate "minMax -1.2e3 400";
boundaryMarks "2500/2500";
plotBoundaryDots false;
