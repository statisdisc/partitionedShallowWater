|   \\  /    A nd           | Web:      http://www.OpenFOAM.org               |
|    \\/     M anipulation  |                                                 |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//Numerical solution
useOperatorSplitting  false;
semiImplicit  true;
offCentre  0.5;
offCentreOpSplit  0.5;

//Physical solution
useCoriolis  false;
useDrag  false;
useDiffusion  false;
useMassTransfer  true;
useBuoyancy  true;
useGravity  false;


solvers
{
    "h|p"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-8;
        relTol          0.01;
    }

    "hFinal|pFinal"
    {
        $p;
        relTol          0;
    }

    "U|UFinal|hU|hUFinal"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-05;
        relTol          0;
    }
    "streamFunction|velPot"
    {
        solver         PCG;
        preconditioner DIC;
        tolerance      1e-7;
        relTol         0;
    }
}

"iterations|PISO|PIMPLE"
{
    nCorrectors     1;
    nUCorrectors    1;
    pRefCell        0;
    pRefValue       0;
}

initialisation
{
    maxIters 20;
}

// ************************************************************************* //
