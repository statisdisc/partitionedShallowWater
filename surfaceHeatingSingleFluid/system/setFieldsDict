FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    // Set cell values
    boxToCell
    {
        box (-2.5e3 -2.5e3 0) (2.5e3 2.5e3 1e2);

        fieldValues
        (
            volScalarFieldValue theta.stable 300
        );
    }
);

