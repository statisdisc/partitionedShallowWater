FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X12c/12c";
projection2D XZ;

FieldsToPlot
(
    //[sigma.buoyant filledContours 0 1 0.05 "$GMTU/colours/white_black.cpt"]
    [sigma.buoyant rawValues 0 1 0.05 "$GMTU/colours/white_black.cpt"]
);

separateLegend true;
region "-5000/5000/0/1e4";
annotate "minMax -4.7e3 400";
boundaryMarks "2500/2500";
plotBoundaryDots false;
