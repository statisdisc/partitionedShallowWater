FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceVectorField;
    location    "0";
    object      Uf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            cyclic;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            cyclic;
        value           uniform (0 0 0);
    }
    top
    {
        type            cyclic;
        value           uniform (0 0 0);
    }
    bottom
    {
        type            cyclic;
        value           uniform (0 0 0);
    }
}


