FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

divSchemes
{
    //default         Gauss cubicUpwindCPCFit 3;
    //default         Gauss vanLeer;
    default         Gauss linearUpwind linearUpwind;
}

interpolationSchemes
{
    default         cubic;
}

gradSchemes
{
    default         Gauss;
}

laplacianSchemes
{
    default         none;
}

fluxRequired
{
    default         no;
}

